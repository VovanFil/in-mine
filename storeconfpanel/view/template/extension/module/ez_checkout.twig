                                        <option value="12" selected="selected">100%</option>
                                       {% else %}
                                        <option value="12">100%</option>
                                       {% endif %}
                                        {% if ez_checkout_cart_width == '6' %}
                                        <option value="6" selected="selected">50%</option>
                                       {% else %}
                                        <option value="6">50%</option>
                                       {% endif %}
                                        {% if ez_checkout_cart_width == '4' %}
                                        <option value="4" selected="selected">33%</option>
                                       {% else %}
                                        <option value="4">33%</option>
                                       {% endif %}
                                        {% if ez_checkout_cart_width == '3' %}
                                        <option value="3" selected="selected">25%</option>
                                       {% else %}
                                        <option value="3">25%</option>
                                       {% endif %}
