                                <label class="col-sm-2 control-label">{{ entry_cart_status }}:</label>
                                <div class="col-sm-3">
                                    <div class="btn-group" data-toggle="buttons">
                                        <label class="btn btn-default{% if ez_checkout_cart_status %} active {% endif %} " title="{{ text_enabled }}">
                                            {% if ez_checkout_cart_status %}
                                            <input type="radio" name="ez_checkout_cart_status" value="1" checked>{{ action_active }}
                                           {% else %}
                                            <input type="radio" name="ez_checkout_cart_status" value="1">{{ action_active }}
                                           {% endif %}
                                        </label>
                                        <label class="btn btn-default{% if not ez_checkout_cart_status %} active {% endif %} " title="{{ text_disabled }}">
                                            {% if ez_checkout_cart_status %}
                                            <input type="radio" name="ez_checkout_cart_status" value="0">{{ action_disable }}
                                           {% else %}
