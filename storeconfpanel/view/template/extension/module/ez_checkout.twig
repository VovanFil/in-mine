                        <li class="active"><a href="#tab-general" data-toggle="tab">{{ text_main_settings }}</a></li>
                        <li><a href="#tab-cart" data-toggle="tab">{{ entry_cart_status }}</a></li>
                        <li><a href="#tab-user" data-toggle="tab">{{ entry_user_status }}</a></li>
                        <li><a href="#tab-payment-address" data-toggle="tab">{{ entry_payment_address_status }}</a></li>
