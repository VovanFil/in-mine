<script type="text/javascript">
    $('select[name=\'ez_checkout_payment_country_default\']').on('change', function() {
        console.log($(this).val());
        if (parseInt($(this).val())) {
            $('select[name=\'ez_checkout_payment_zone_id_default\']').removeAttr('disabled');
        } else {
            $('select[name=\'ez_checkout_payment_zone_id_default\']').attr('disabled', 'disabled');
        }
        $.ajax({
            url: '/index.php?route=checkout/checkout/country&country_id=' + this.value,
            dataType: 'json',
            beforeSend: function() {
                $('select[name=\'ez_checkout_payment_country_default\']').after(' <i class="fa fa-circle-o-notch fa-spin"></i>');
            },
            complete: function() {
                $('.fa-spin').remove();
            },
            success: function(json) {
                {% if ez_checkout_payment_country_default %}
                html = '<option value="0">{{ text_not_checked }}</option>';
               {% else %}
                html = '<option value="0" selected>{{ text_not_checked }}</option>';
               {% endif %}

                if (json['zone'] && json['zone'] != '') {
                    for (i = 0; i < json['zone'].length; i++) {
                        html += '<option value="' + json['zone'][i]['zone_id'] + '"';

                        if (json['zone'][i]['zone_id'] == '{{ ez_checkout_payment_zone_id_default }}') {
                            html += ' selected="selected"';
                        }
                        html += '>' + json['zone'][i]['name'] + '</option>';
                    }
                }

                $('select[name=\'ez_checkout_payment_zone_id_default\']').html(html);
            },
            error: function(xhr, ajaxOptions, thrownError) {
                alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
            }
        });
    });

    $('select[name=\'ez_checkout_payment_country_default\']').trigger('change');
    //
//--></script>

